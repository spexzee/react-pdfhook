{
  "name": "@spexzee/react-pdfhook",
  "version": "1.0.2",
  "description": "React hook for generating PDF documents from components",
  "main": "dist/index.js",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prerelease": "npm run clean && npm run build",
    "prepare": "npm run build",
    "clean": "rimraf dist"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./usePdfGenerator": {
      "import": "./dist/usePdfGenerator.js",
      "require": "./dist/usePdfGenerator.js",
      "types": "./dist/usePdfGenerator.d.ts"
    }
  },
  "keywords": [
    "react",
    "pdf",
    "convert",
    "document",
    "generator",
    "hook",
    "page-break",
    "add-image",
    "add-element"
  ],
  "author": "Spexzee | Abu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spexzee/react-pdfhook.git"
  },
  "bugs": {
    "url": "https://github.com/spexzee/react-pdfhook/issues"
  },
  "homepage": "https://github.com/spexzee/react-pdfhook#readme",
  "license": "MIT",
  "type": "commonjs",
  "peerDependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "react": ">=16.8.0"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.2"
  }
}
